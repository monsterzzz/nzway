<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.monster.nzway.mapper.AvatarMapper">

    <resultMap id="AvatarResultMap" type="com.monster.nzway.domain.Avatar">
        <id column="id" property="id"/>
        <result column="url" property="url"/>
    </resultMap>

    <select id="all" resultType="com.monster.nzway.domain.Avatar">
        select  * from avatar;
    </select>
</mapper>